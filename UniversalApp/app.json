{
  "expo": {
    "name": "Riwayat",
    "slug": "UniversalApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_logo.png",
    "scheme": "universalapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.riwayat.partner",
      "adaptiveIcon": {
        "backgroundColor": "#600E10",
        "foregroundImage": "./assets/images/app_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#600E10",
          "dark": {
            "backgroundColor": "#600E10"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}